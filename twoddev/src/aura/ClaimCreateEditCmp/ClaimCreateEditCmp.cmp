<aura:component extends="c.CruderCmp" controller="ClaimCreateEditExt">
    
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    
    <!-- aura system error handeler -->
    <aura:handler event="aura:systemError" action="{!c.handleAuraError}"/>
    
    <!-- component variables -->
    <aura:attribute name="userLangId" type="String" default="en_US"/>
    <aura:attribute name="accordionColor" type="String" default=""/>
    <aura:attribute name="claimPageHeading" type="String" default="{!$Label.c.Claim_Create_Heading}"/>
    <aura:attribute name="isEdit" type="Boolean"/>
    <aura:attribute name="error" type="Object"/>
    <aura:attribute name="modalAndElementDisplayBool" type="Object"/>
    <aura:attribute name="spinnerBool" type="Object"/>
    <aura:attribute name="currentClaim" type="Object"/>
    <aura:attribute name="accordionValidationIndicator" type="Object"/>
    <aura:attribute name="modalData" type="Object"/>
    <aura:attribute name="relatedListData" type="Object"/>
    <aura:attribute name="customSettingManager" type="Object"/>
    <aura:attribute name="site" type="String" default=''/>
    <aura:attribute name="claimListViewId" type="String" default=''/>
    <aura:attribute name="fieldModInitEditBoolean" type="Boolean" default='false'/>
    <aura:attribute name="acknowledgedForValidate" type="Boolean" default='false'/>
    <aura:attribute name="upcomingScheduleList" type="String"/>
    <aura:attribute name="inventoryScheduleDescription" type="String"/>
    <aura:attribute name="inventoryScheduleDate" type="String"/>
    <aura:attribute name="scheduleSetValue" type="String" default=''/>
    <aura:attribute name="DOF" type="String" default="Date Of Failure"/>
    <aura:attribute name="gracePeriod" type="Integer" default="0"/>
    <aura:attribute name="finalScehdule" type="Object"/>
    
    <!-- added by Dharmil for Fault Code Job Code logic -->
    <aura:attribute name="faultCodeListForJobCode" type="String"/>
    <aura:attribute name="isModelParentJobCodeList" type="Boolean"/>
    <aura:attribute name="isModelParentJobCodeCustomSetting" type="Boolean"/>
    
    <!--18-NOV-2016-->
    
    <!-- page form variables -->
    <aura:attribute name="product" type="Object"/>
    <aura:attribute name="inventory" type="Object"/>
    <aura:attribute name="dealer" type="Object"/>
    <aura:attribute name="applicablePolicy" type="Object"/>
    <aura:attribute name="paymentDefinition" type="Object"/>
    <aura:attribute name="rate" type="Object"/>
    
    <aura:attribute name="isDealerEditable" type="Boolean" default="true"/>
    <aura:attribute name="isInventoryScheduleEditable" type="Boolean" default="true"/>
    <aura:attribute name="selectedSchedule" type="String" />
    <!--18-NOV-2016-->
    <aura:attribute name="causalPart" type="Object"/>
    <aura:attribute name="installedPartRecordTypeList" type="Object[]"/>
    <aura:attribute name="removedPartRecordTypeList" type="Object[]"/>
    
    <!-- selected variables -->
    <aura:attribute name="selectedJobCode" type="Object"/>
    <aura:attribute name="selectedNonSerializedInstalledPart" type="Object"/>
    <aura:attribute name="selectedSerializedInstalledPart" type="Object"/>
    <aura:attribute name="selectedNonSerializedRemovedPart" type="Object"/>
    <aura:attribute name="selectedSerializedRemovedPart" type="Object"/>
    <aura:attribute name="selectedNONOEMPart" type="Object"/>
    
    <aura:attribute name="claimType" type="String" default=""/>
    <aura:attribute name="failureRepairDateDiffBig" type="Boolean"/>
    <aura:attribute name="claimPicklist" type="Object[]"/>
    <aura:attribute name="faultCode" type="String"/>
    <aura:attribute name="faultCodeList" type="Object[]"/>
    
    <aura:attribute name="claimServiceInfoList" type="Object[]"/>
    <aura:attribute name="installedPartList" type="Object[]"/>
    <aura:attribute name="removedPartList" type="Object[]"/>
    <aura:attribute name="nonOEMPartList" type="Object[]"/>
    <aura:attribute name="partsToBeShippedList" type="Object[]"/>
    <aura:attribute name="inventoryScheduleList" type="Object[]"/>
    
    <aura:attribute name="unitType" type="String" default=""/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <!-- picklist values-->
    <aura:attribute name="claimRecordTypeList" type="Object[]"/>
    <aura:attribute name="hostNonHostList" type="Object[]"/>
    <aura:attribute name="smrReasonList" type="Object[]"/>
    <aura:attribute name="causedByList" type="Object[]"/>
    <aura:attribute name="faultFoundList" type="Object[]"/>
    
    <!-- campaign member variable for field mod claim -->
    <aura:attribute name="campaignMember" type="Object"/>
    <aura:attribute name="claimTemp" type="Object"/>
    
    <!-- value selected in picklist-->
    <aura:attribute name="claimCustomRecordTypeName" type="String" default="Serialized"/>
    <aura:attribute name="hostNonHostType" type="Object"/>
    <aura:attribute name="smrReason" type="Object"/>
    
    <!-- date fields-->
    <aura:attribute name="purchaseDate" type="Date" default="2016-10-10"/>
    <aura:attribute name="repairDate" type="Date" default="2016-10-10"/>
    <aura:attribute name="failureDate" type="Date" default="2016-10-10"/>
    
    <!-- change handeler for date fields, needed because uidatepicker does not fire change event-->
    <aura:handler name="change" value="{!v.purchaseDate}" action="{!c.purchaseDateChangeHandeler}"/>
    <aura:handler name="change" value="{!v.repairDate}" action="{!c.repairDateChangeHandeler}"/>
    <aura:handler name="change" value="{!v.failureDate}" action="{!c.failureDateChangeHandeler}"/>
    
    <!--Aditya integration -->
    <!--Custom setting Values for partPrice Integrtion-->
    <aura:attribute name="partpriceSettings" type="Boolean"/>
    <aura:attribute name="partPriceException" type="Boolean"/>
    <aura:attribute name="partPriceErrorMsg" type="String"/>
    <aura:attribute name="partPriceNotFound" type="Boolean" default="false"/>
    <!--/Aditya integration -->
    
    <aura:attribute name="description"
                    type="string"
                    default="Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries, but also the leap into electronic typesetting, remaining essentially unchanged. It was popularised in the 1960s with the release of Letraset sheets containing Lorem Ipsum passages, and more recently with desktop publishing software "/>
    
    <div class="main-body">
        <div class="alertbox">{!v.body}</div>
        <div class="page-spinner-container">
            <aura:if isTrue="{!v.spinnerBool.pageSpinner}">
                <div class="slds-spinner_container">
                    <div class="slds-spinner--brand slds-spinner slds-spinner--medium" role="alert">
                        <span class="slds-assistive-text">Loading</span>
                        <div class="slds-spinner__dot-a"></div>
                        <div class="slds-spinner__dot-b"></div>
                    </div>
                </div>
            </aura:if>
        </div>
        
        <header class="slds-text-heading--large slds-align--absolute-center header">
            {!v.claimPageHeading}
        </header>
        
        <div class="containerC">
            
            <div class="accordion-container">
                
                <!--///added on 17-NOV-2016///-->
                <lightning:button name="accordion-c-1-cancel" variant="destructive" label="Cancel claim and return" onclick="{! c.cancel }"/>
                <div class="form-break"/>
                
                <!-- Accordion 1 - Claim Basic Info -->
                <div class="accordion">
                    <label class="accordion-heading slds-text-heading--medium" for="accordion-c-1" id="assist">
                        {!$Label.c.Claim_Section_1}
                        <span class="open-close"></span>
                    </label>
                    <input type="checkbox" class="design-checkbox" id="accordion-c-1" aura:id="accordion-c-1" onchange="{!c.validateAccordionOpen}"/>
                    
                    <div class="accordion-body tab-open" aura:id="accordion-c-1-body">
                        <div class="form">
                            <div class="form-body">
                                
                                <div class="slds-grid slds-wrap slds-grid">
                                    <div class="slds-size--12-of-12 slds-medium-size--6-of-12 slds-large-size--6-of-12 slds-p-around--medium">
                                        
                                        <ui:inputSelect aura:id="claimCustomRecordTypeName" disabled="{!v.isEdit}" class="slds-input select-box-padder" label="{!$Label.c.Claim_Record_Type}" value="{!v.claimCustomRecordTypeName}" change="{!c.basicInfoHandeler}">
                                            <aura:if isTrue="{!v.claimRecordTypeList.length!=0}">
                                                <aura:iteration items="{!v.claimRecordTypeList}" var="recordType">
                                                    <ui:inputSelectOption text="{!recordType.value}" label="{!recordType.label}" value="{!(v.claimCustomRecordTypeName == recordType.value)}"/>
                                                </aura:iteration>
                                            </aura:if>
                                        </ui:inputSelect>
                                    </div>
                                    
                                </div>
                                
                                <aura:if isTrue="{!(v.claimCustomRecordTypeName!='Claim Template')}">
                                    
                                    <div class="slds-grid slds-wrap slds-grid ">
                                        <aura:if isTrue="{!(v.claimCustomRecordTypeName!='Claim Template')}">
                                            <div class="slds-size--12-of-12 slds-medium-size--6-of-12 slds-large-size--6-of-12 slds-p-around--medium">
                                                <!--18-NOV-2016-->
                                                
                                                <c:LookupCmp aura:id="dealer"
                                                             objectType="Account"
                                                             objectName="{!v.currentClaim.Account__r.Name}"
                                                             disabled="{!(!v.isDealerEditable)}"
                                                             class="slds-input required"
                                                             label="{!$Label.c.Dealer}"
                                                             onBlur="{!c.fillError}"
                                                             where="AND Warranty_Account_Type__c='Dealer'"
                                                             onLookupSelect="{!c.basicInfoHandeler}"
                                                             minCharacters="{!v.customSettingManager.Minimum_Lookup_Characters}"
                                                             placeholder="{!$Label.c.Dealer_Name_Placeholder}"/>
                                                
                                                <aura:if isTrue="{!v.error.accordion1.fieldError.dealer!=''}">
                                                    <div class="field-error">
                                                        {!v.error.accordion1.fieldError.dealer}
                                                    </div>
                                                </aura:if>
                                            </div>
                                        </aura:if>
                                        
                                        <aura:if isTrue="{!(v.claimCustomRecordTypeName=='Serialized' || v.claimCustomRecordTypeName=='Field Modification' ||v.claimCustomRecordTypeName=='Preventive Maintenance')}">
                                            <div class="slds-size--12-of-12 slds-medium-size--6-of-12 slds-large-size--6-of-12 slds-p-around--medium"> 
                                                <c:LookupCmp aura:id="inventory"
                                                             objectType="Inventory__c"
                                                             disabled="{!v.isEdit}"
                                                             objectName="{!v.currentClaim.Inventory__r.Name}"
                                                             fields="Id,name,Item__c,Units_Run__c,Item__r.Name,Item__r.Description__c,
                                                                     Item__r.Parent_Product__r.Id,Item__r.Parent_Product__r.Name,Item__r.Parent_Product__r.Description__c,
                                                                     Item__r.Parent_Product__r.Parent_Product__r.Id,
                                                                     Item__r.Parent_Product__r.Type__c,
                                                                     Item__r.Item_Type__c,Item__r.Units_of_Measure__c,Install_Date__c"
                                                             class="slds-input required"
                                                             onBlur="{!c.fillError}"
                                                             onLookupSelect="{!c.basicInfoHandeler}"
                                                             minCharacters="{!v.customSettingManager.Minimum_Lookup_Characters}"
                                                             label="{!$Label.c.Inventory}" 
                                                             placeholder="{!$Label.c.Inventory_Name_Placeholder}"/>
                                                <aura:if isTrue="{!v.error.accordion1.fieldError.inventory!=''}">
                                                    <div class="field-error">
                                                        {!v.error.accordion1.fieldError.inventory}
                                                    </div>
                                                </aura:if>
                                            </div>
                                        </aura:if>
                                        <aura:if isTrue="{!(v.claimCustomRecordTypeName=='Non-Serialized')}">
                                            <div class="slds-size--12-of-12 slds-medium-size--6-of-12 slds-large-size--6-of-12 slds-p-around--medium">
                                                <c:LookupCmp aura:id="product"
                                                             objectType="Warranty_Product__c"
                                                             objectName="{!v.currentClaim.Warranty_Product__r.Name}"
                                                             fields="Id,Name,Parent_Product__r.Name,Units_of_Measure__c,Item_Type__c,Description__c,Parent_Product__r.Description__c,Product_Name__c"
                                                             where="AND Track_Type__c='Non-Serialized'"
                                                             disabled="{!v.isEdit}"
                                                             class="slds-input required"
                                                             onBlur="{!c.fillError}"
                                                             onLookupSelect="{!c.basicInfoHandeler}"
                                                             minCharacters="{!v.customSettingManager.Minimum_Lookup_Characters}"
                                                             label="{!$Label.c.Item_Number}"
                                                             placeholder="{!$Label.c.Product_Name_Placeholder}"/>
                                                <aura:if isTrue="{!v.error.accordion1.fieldError.product!=''}">
                                                    <div class="field-error">
                                                        {!v.error.accordion1.fieldError.product}
                                                    </div>
                                                </aura:if>
                                            </div>
                                        </aura:if>
                                    </div>
                                    
                                    
                                    
                                    <div class="slds-grid slds-wrap slds-grid ">
                                        
                                        <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                            <ui:inputText value="{!v.claimType}" class="slds-input output" disabled="true" label="{!$Label.c.Claim_Type}"/>
                                        </div>
                                        <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                            <ui:inputText value="{!(v.product.Parent_Product__r.Name || v.inventory.Item__r.Parent_Product__r.Name)}" class="slds-input output" disabled="true" label="{!$Label.c.Product_Name}"/>
                                        </div>
                                        <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                            <ui:inputText aura:id="modelDescription" value="{!(v.product.Parent_Product__r.Description__c || v.inventory.Item__r.Parent_Product__r.Description__c)}" class="slds-input output" disabled="true" label="{!$Label.c.Model_Description}"/>
                                        </div>
                                    </div>
                                    <div class="slds-grid slds-wrap slds-grid ">
                                        <div class="slds-size--12-of-12 slds-medium-size--12-of-12 slds-large-size--12-of-12 slds-p-around--medium">
                                            <label class="slds-checkbox">
                                                <ui:inputCheckbox aura:id="preAuthorization" value="{!v.currentClaim.Request_SMR__c}" change="{!c.basicInfoHandeler}"/>
                                                <span>{!$Label.c.Pre_authorization}&nbsp;&nbsp;</span>
                                                <span class="slds-checkbox--faux border-highlighten"></span>
                                            </label>
                                        </div>
                                    </div>
                                    
                                    <aura:if isTrue="{!v.currentClaim.Request_SMR__c}">
                                        <div class="slds-grid slds-wrap slds-grid ">
                                            <div class="slds-size--12-of-12 slds-medium-size--12-of-12 slds-large-size--12-of-12 slds-p-around--medium">
                                                <ui:inputSelect class="slds-input select-box-padder" label="{!$Label.c.Reasons_for_Pre_authorization}" aura:id="reasonsForPreAuth" change="{!c.basicInfoHandeler}"/>
                                            </div>
                                        </div>
                                    </aura:if>
                                    
                                    <div class="slds-grid slds-wrap slds-grid">
                                        <aura:if isTrue="{!(v.claimCustomRecordTypeName=='Non-Serialized')}">
                                            <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                                <ui:inputSelect class="slds-input select-box-padder" label="{!$Label.c.Host_Non_Host}" aura:id="hostNonHost" change="{!c.basicInfoHandeler}"/>
                                            </div>
                                            <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                                <aura:if isTrue="{!(!v.isEdit)}">
                                                    <ui:inputDate aura:id="purchaseDate" class="slds-input required" label="{!$Label.c.Date_of_Purchase}" value="{!v.purchaseDate}" displayDatePicker="true"/>
                                                    <aura:if isTrue="{!v.error.accordion1.fieldError.purchaseDate!=''}">
                                                        <div class="field-error">
                                                            {!v.error.accordion1.fieldError.purchaseDate}
                                                        </div>
                                                    </aura:if>
                                                    <aura:set attribute="else">
                                                        <label class="customLabel">
                                                            <span>{!$Label.c.Date_of_Purchase}</span>
                                                        </label>
                                                        <ui:outputDate class="slds-input output" value="{!v.purchaseDate}" />
                                                    </aura:set>
                                                </aura:if>
                                            </div>
                                        </aura:if>
                                        
                                        <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                            <aura:if isTrue="{!(!v.isEdit)}">
                                                <ui:inputDate aura:id="failureDate" class="slds-input required" label="{!v.DOF}" value="{!v.failureDate}" updateOn="change" blur="{!c.fetchSchedule}" select="{!c.fillError}" displayDatePicker="true"/>
                                                <aura:if isTrue="{!v.error.accordion1.fieldError.failureDate!=''}">
                                                    <div class="field-error">
                                                        {!v.error.accordion1.fieldError.failureDate}
                                                    </div>
                                                </aura:if>
                                                <aura:set attribute="else">
                                                    <label class="customLabel">
                                                        <span>{!v.DOF}</span>
                                                    </label>
                                                    <ui:outputDate class="slds-input output" value="{!v.failureDate}" />
                                                </aura:set>
                                            </aura:if>
                                        </div>
                                        
                                        <aura:if isTrue="{!(v.claimCustomRecordTypeName!='Preventive Maintenance')}">
                                        <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                            
                                            <aura:if isTrue="{!(!v.isEdit)}">
                                            	<ui:inputDate aura:id="repairDate" class="slds-input required" label="{!$Label.c.Date_of_Repair}" value="{!v.repairDate}" displayDatePicker="true"/>
                                                <aura:if isTrue="{!v.error.accordion1.fieldError.repairDate!=''}">
                                                    <div class="field-error">
                                                        {!v.error.accordion1.fieldError.repairDate}
                                                    </div>
                                                </aura:if>
                                                <aura:set attribute="else">
                                                    <label class="customLabel">
                                                        <span>{!$Label.c.Date_of_Repair}</span>
                                                    </label>
                                                    <ui:outputDate class="slds-input output" value="{!v.repairDate}" />
                                                </aura:set>
                                            </aura:if>
                                            
                                        </div>
                                        </aura:if>
                                        
                                        
                                        <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                            <aura:if isTrue="{!(v.claimCustomRecordTypeName!='Non-Serialized')}">
                                            <ui:inputNumber aura:id="unitsUsage" value="{!v.currentClaim.Units_Run__c}" class="slds-input required" label="{!$Label.c.Units_Usage  + v.unitType }" change="{!c.fetchSchedule}" blur="{!c.fillError}" updateOn="keyup"/>
                                            <aura:if isTrue="{!v.error.accordion1.fieldError.unitsUsage!='' || v.error.accordion1.fieldError.unitsUsage!=0}">
                                                <div class="field-error">
                                                    {!v.error.accordion1.fieldError.unitsUsage}
                                                </div>
                                            </aura:if>
                                            <aura:set attribute="else">
                                            	<ui:inputNumber aura:id="unitsUsage" value="{!v.currentClaim.Units_Run__c}" class="slds-input" label="{!$Label.c.Units_Usage  + v.unitType }" updateOn="keyup"/>
                                            </aura:set>
                                        </aura:if>
                                        </div>
                                        
                                    </div>
                                    
                                    <aura:if isTrue="{!(v.claimCustomRecordTypeName=='Field Modification')}">
                                        <div class="slds-grid slds-wrap slds-grid ">
                                            <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                                <ui:inputSelect class="slds-input select-box-padder" disabled="{!v.isEdit}" label="{!$Label.c.Campaign}" aura:id="campaignMember" change="{!c.basicInfoHandeler}"/>
                                            </div>
                                            <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                                <ui:inputDate class="slds-input select-box-padder output" value="{!v.campaignMember.Service_Campaign__r.Start_Date__c}" disabled="true" label="{!$Label.c.Campaign_Start_Date}" aura:id="campaignStartDate"/>
                                            </div>
                                            <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium">
                                                <ui:inputDate class="slds-input select-box-padder output" value="{!v.campaignMember.Service_Campaign__r.End_Date__c}" disabled="true" label="{!$Label.c.Campaign_End_Date}" aura:id="campaignEndDate"/>
                                            </div>
                                        </div>
                                    </aura:if>
                                    
                                    <aura:if isTrue="{!v.failureRepairDateDiffBig}">
                                        <div class="slds-grid slds-wrap slds-grid">
                                            <div class="slds-size--12-of-12 slds-medium-size--12-of-12 slds-large-size--12-of-12 slds-p-around--medium">
                                                <ui:inputTextArea aura:id="reasonForDelay"
                                                                  value="{!v.currentClaim.Delay_Reason__c}"
                                                                  label="{!$Label.c.Reason_for_Delay}"
                                                                  class="slds-input required"
                                                                  resizable="false"
                                                                  required="true"
                                                                  blur="{!c.fillError}"
                                                                  change="{!c.fillError}"
                                                                  updateOn="keyup"/>
                                                <aura:if isTrue="{!v.error.accordion1.fieldError.reasonForDelay!=''}">
                                                    <div class="field-error">
                                                        {!v.error.accordion1.fieldError.reasonForDelay}
                                                    </div>
                                                </aura:if>
                                            </div>
                                        </div>
                                    </aura:if>
                                    
                                </aura:if>
                                
                                <aura:if isTrue="{!(v.claimCustomRecordTypeName=='Claim Template-x')}">
                                    <div class="slds-grid slds-wrap slds-grid">
                                        <div class="slds-size--12-of-12 slds-medium-size--6-of-12 slds-large-size--6-of-12 slds-p-around--medium">
                                            <c:LookupCmp aura:id="rate"
                                                         objectType="Rates_Details__c"
                                                         objectName="{!v.currentClaim.Rate__r.Name}"
                                                         fields="Id,Name"
                                                         disabled="{!v.isEdit}"
                                                         class="slds-input"
                                                         onBlur="{!c.fillError}"
                                                         onLookupSelect="{!c.basicInfoHandeler}"
                                                         minCharacters="{!v.customSettingManager.Minimum_Lookup_Characters}"
                                                         label="{!$Label.c.Rate}"
                                                         placeholder="{!$Label.c.Rate_Detail_Name_Placeholder}"/>
                                        </div>
                                        <div class="slds-size--12-of-12 slds-medium-size--6-of-12 slds-large-size--6-of-12 slds-p-around--medium">
                                            <c:LookupCmp aura:id="paymentDefinition"
                                                         objectType="Payment_Definitions__c"
                                                         objectName="{!v.currentClaim.Claim_Template_Payment_Definition__r.Name}"
                                                         fields="Id,Name"
                                                         disabled="{!v.isEdit}"
                                                         class="slds-input"
                                                         onBlur="{!c.fillError}"
                                                         onLookupSelect="{!c.basicInfoHandeler}"
                                                         minCharacters="{!v.customSettingManager.Minimum_Lookup_Characters}"
                                                         label="{!$Label.c.Payment_Definition}"
                                                         placeholder="{!$Label.c.Payment_Definition_Placeholder}"/>
                                        </div>
                                    </div>
                                </aura:if>
                                
                                <!-- Logically second accordion-->
                                <aura:if isTrue="{!(v.currentClaim.Inventory__c != '' &amp;&amp; v.claimCustomRecordTypeName == 'Preventive Maintenance')}">
                                        <div class="slds-grid slds-wrap slds-grid ">
                                            <aura:if isTrue="{!!v.isEdit}">
                                                <div class="slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 slds-p-around--medium" >
                                                    <ui:inputSelect value="{!v.selectedSchedule}"  blur="{!c.fillError}" class="slds-input select-box-padder" disabled="{!v.isEdit}" label="Scheduler" aura:id="upcomingScheduler"/>
                                                    
